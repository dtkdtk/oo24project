{
  "include": [
    "One3L.ts",
  ],
  "exclude": ["**/*.js"],
  "compilerOptions": {
    "allowJs": false,
    "checkJs": false,
    "outDir": "./",
    "noEmit": false,

    "resolveJsonModule": false,
    "module": "None",
    "moduleResolution": "node",
    "target": "ES6",
    "lib": ["ES6"],

    "strict": false,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
  }
}